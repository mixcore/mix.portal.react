  },
  "scripts": {
    "dev": "next dev",
    "custom-server": "node server.js",
    "dev:https": "node server.js",
    "generate-certs": "node generate-certificates.js",
    "generate-certs:node": "node generate-self-signed-certs.js",
    "dev:setup": "npm run generate-certs && npm run dev:https",
    "dev:setup:node": "npm run generate-certs:node && npm run dev:https",
    "build": "next build",
    "build:optimized": "NODE_OPTIONS='--max_old_space_size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write .",
    "format:check": "prettier -c -w .",
    "prepare": "husky",
    "mock-api": "node mock-api.js",
    "dev:mock": "concurrently \"pnpm dev\" \"pnpm mock-api\""
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "!src/app/dashboard/mixdb/databases/[id]/data/page.tsx": [
      "echo 'Skipping problematic file'"
    ]
  },
